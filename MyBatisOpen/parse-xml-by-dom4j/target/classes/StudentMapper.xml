<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Student's Insert">
    <insert id="insertStu">
        insert into t_student (id,name,xh,sex) values (null,#{name},#{xh},#{sex});
    </insert>
    <delete id="deleteStu">
        delete from t_student where id = #{id}
    </delete>
</mapper>