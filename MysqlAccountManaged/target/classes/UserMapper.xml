<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hbsfdxwlxy.competition.managed.dao">
    <select id="selectUser" resultType="com.hbsfdxwlxy.competition.managed.pojo.User">
        select username,password from t_user where username = #{username} and password = #{password};
    </select>
    <insert id="insertUser">
        insert into t_user(username,password) values(#{username},#{password});
    </insert>
</mapper>